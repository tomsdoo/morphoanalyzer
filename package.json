  ],
  "author": "tom",
  "license": "MIT",
  "dependencies": {
    "kuromoji": "^0.1.2"
  },
  "devDependencies": {
    "@types/kuromoji": "^0.1.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.0.4",
    "mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
